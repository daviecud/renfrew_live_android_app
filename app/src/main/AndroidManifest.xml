<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.renfrewlive">

    <application
        android:allowBackup="true"
        android:icon="@drawable/renfrew_arms"
        android:label="@string/app_name"
        android:roundIcon="@drawable/renfrew_arms"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LocalBusinessActivity"></activity>
        <activity android:name=".DataActivity" />
        <activity android:name=".QuotesActivity" />
        <activity android:name=".ScratchActivity" />
        <activity android:name=".LostFoundActivity" />
        <activity android:name=".BusinessActivity" />
        <activity android:name=".UsefulInfoActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>